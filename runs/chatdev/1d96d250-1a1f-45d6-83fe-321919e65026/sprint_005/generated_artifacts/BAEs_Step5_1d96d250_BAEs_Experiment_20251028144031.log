SessionLocal = sessionmaker(autocommit=False, autoflush=False, bind=engine)
Base = declarative_base()
def get_db() -> Session:
    '''
    Create a new database session.
    '''
    db = SessionLocal()
    try:
        yield db
    finally:
        db.close()
def init_db() -> None:
    '''
    Initialize the database and create tables.
    '''
    Base.metadata.create_all(bind=engine)
