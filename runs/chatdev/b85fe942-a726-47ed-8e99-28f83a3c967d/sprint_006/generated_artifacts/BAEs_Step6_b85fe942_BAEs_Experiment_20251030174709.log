        course_name = self.entry_course_name.get()
        course_level = self.entry_course_level.get()
        teacher_id = self.entry_teacher_id.get()  # Get teacher ID from input
        if course_name and course_level:
            if teacher_id.isdigit():  # Validate teacher ID
                response = requests.post("http://127.0.0.1:8000/courses/", json={"name": course_name, "level": course_level, "teacher_id": int(teacher_id)})
                if response.status_code == 201:
