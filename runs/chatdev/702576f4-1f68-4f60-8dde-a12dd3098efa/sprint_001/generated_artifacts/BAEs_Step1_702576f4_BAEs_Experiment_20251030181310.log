            students = response.json()
            student_list = "\n".join([f"{student['id']}: {student['name']}" for student in students])
            messagebox.showinfo("Students", student_list)
        else:
            messagebox.showerror("Error", "Failed to retrieve students.")

    def run(self):
        self.root.mainloop()
