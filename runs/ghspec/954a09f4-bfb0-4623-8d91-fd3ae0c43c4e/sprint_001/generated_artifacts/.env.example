# .env.example

# Environment configuration for Student Management Application

# Database configuration
DATABASE_URL=sqlite:///students.db  # SQLite Database file location

# Flask configuration
FLASK_ENV=development                # Environment mode (development or production)
FLASK_APP=app.py                     # Application entry point

# Secret key for session management
SECRET_KEY=your_secret_key_here      # Change this to a strong secret key for production

# Other optional configuration options can be added below
# For example, you can set logging levels, CORS settings, etc.