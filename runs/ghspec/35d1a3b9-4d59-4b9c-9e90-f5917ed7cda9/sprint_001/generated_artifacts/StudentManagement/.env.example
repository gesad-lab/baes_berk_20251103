# This is the content for the StudentManagement/.env.example file.

# Database configuration
# SQLite URI for database
DATABASE_URI=sqlite:///students.db

# Flask configuration
# Secret key for session management
SECRET_KEY=your_secret_key_here

# Set to 'development' for local testing, 'production' for live deployment
FLASK_ENV=development

# Enable or disable debug mode
DEBUG=True

# Logging level (options: DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=DEBUG

# You may also include other settings that might be necessary for your application
# but these parameters are the primary for the student management system.